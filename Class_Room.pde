class Room {
  String roomDescription;
  HashMap<String, Room> exits;
  ArrayList<Item> items;
  ArrayList<Enemy> enemyList;
  
  Room(String roomDescription, HashMap<String, Room> exits, ArrayList<Item> items, ArrayList<Enemy> enemyList){
